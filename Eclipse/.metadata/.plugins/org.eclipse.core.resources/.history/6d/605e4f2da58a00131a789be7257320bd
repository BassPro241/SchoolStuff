package pistachio6;

import java.util.ArrayList;

public class Main
{
    
    public static void main(String[] args) {
        ArrayList<String> list = new ArrayList<String>();
        
        list.add("cookies");
        list.add("nachos");
        list.add("chips");
        list.add("trail mix");
        list.add("celery");
        
        for (String s : list)
        {
            if (s.length() < 7 && s.length() > 4)
                System.out.print(s.toUpperCase() + " ");
        }
    }
}
